{
  "name": "@skypager/web",
  "version": "0.4.12",
  "description": "",
  "author": "Jon Soeder <jon@chicago.com>",
  "main": "lib/index.js",
  "browser": "lib/skypager-runtimes-web.min.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:lib && yarn build:web",
    "docs:api": "skypager generate-api-docs",
    "build:lib": "cross-env NODE_ENV=production babel --comments --source-maps --config-file ./babelrc -d lib src",
    "build:web": "skypager build --app-only --include-unminified",
    "prepare": "yarn docs:api && yarn build",
    "postbuild": "skypager hash-build"
  },
  "dependencies": {
    "@skypager/runtime": "^0.4.12"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/skypager/skypager"
  },
  "homepage": "https://skypager.io",
  "gitHead": "3afe91f6ff7fd3bff7fe47a351619d5281155cd9",
  "keywords": [],
  "contributors": [],
  "skypager": {
    "category": "runtimes",
    "projectType": "runtime",
    "buildFolder": "lib",
    "webpack": {
      "build": "webpack.config.js",
      "libraryName": "skypager",
      "appName": "skypager-runtimes-web"
    }
  },
  "babelRc": {
    "presets": ["react-app"]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
