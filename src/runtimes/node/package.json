{
  "name": "@skypager/node",
  "version": "0.4.1",
  "description": "the skypager node runtime",
  "author": "Jon Soeder <jon@chicago.com>",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel --comments --source-maps -d lib src",
    "postbuild": "skypager hash-build",
    "docs:api": "skypager generate-api-docs",
    "watch": "babel --watch --comments --source-maps -d lib src",
    "console": "skypager console",
    "prepare": "yarn docs:api && yarn build",
    "test": "mocha --require @babel/register --file test/test.js --timeout 10000 test/**/*.spec.js",
    "test:watch": "skypager test --webpack-config webpack.config.test.js --watch"
  },
  "license": "MIT",
  "dependencies": {
    "@skypager/features-file-manager": "^0.4.1",
    "@skypager/features-module-manager": "^0.4.1",
    "@skypager/features-package-manager": "^0.4.1",
    "@skypager/helpers-client": "^0.4.0",
    "@skypager/helpers-repl": "^0.4.0",
    "@skypager/helpers-server": "^0.4.0",
    "@skypager/runtime": "^0.4.0",
    "bluebird": "^3.5.0",
    "child-process-promise": "^2.2.1",
    "cli-spinner": "^0.2.8",
    "cli-table": "^0.3.1",
    "colors": "^1.3.2",
    "cross-env": "^5.2.0",
    "detect-port": "^1.2.1",
    "figlet": "^1.2.0",
    "find-up": "^2.1.0",
    "fs-extra-promise": "^0.4.1",
    "gaze": "^1.1.2",
    "micromatch": "^3.1.4",
    "mime": "^2.4.0",
    "minimatch": "^3.0.3",
    "node-emoji": "^1.8.1",
    "opn": "^5.1.0",
    "pretty-cli": "^0.0.14",
    "prompt": "^1.0.0",
    "semver": "^5.3.0",
    "winston": "^2.3.1"
  },
  "skypager": {
    "webpack": {
      "build": "webpack.config.js",
      "scopeModules": false
    },
    "category": "runtimes"
  },
  "gitHead": "3afe91f6ff7fd3bff7fe47a351619d5281155cd9",
  "keywords": [],
  "contributors": []
}
