{
  "name": "@skypager/portfolio-manager",
  "version": "0.3.0",
  "description": "the skypager portfolio manager",
  "author": "Jon Soeder <jon@chicago.com>",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel --comments --source-maps -d lib src",
    "docs:api": "skypager generate-api-docs",
    "watch": "babel --watch --comments --source-maps -d lib src",
    "console": "skypager console",
    "prepare": "yarn build && yarn docs:api",
    "test": "mocha --require @babel/register --file test/test.js --timeout 10000 test/**/*.spec.js",
    "test:watch": "skypager test --webpack-config webpack.config.test.js --watch"
  },
  "license": "MIT",
  "dependencies": {
    "@skypager/features-file-manager": "^0.3.0",
    "@skypager/features-module-manager": "^0.3.0",
    "@skypager/features-package-manager": "^0.3.0",
    "@skypager/node": "^0.3.0"
  },
  "skypager": {
    "webpack": {
      "build": "webpack.config.js",
      "scopeModules": false
    },
    "category": "tools"
  },
  "gitHead": "4f161bd5a57a5d5969d19cf14b0e712be636467c",
  "keywords": [],
  "contributors": []
}
