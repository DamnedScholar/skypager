{
  "name": "@skypager/sheets-server",
  "version": "0.1.0",
  "description": "a google sheets api server",
  "main": "lib/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development skypager start --esm",
    "prestart": "yarn build",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "watch": "skypager watch",
    "console": "skypager console",
    "prepare": "skypager build --force",
    "pretest": "yarn build",
    "test": "cross-env NODE_ENV=test skypager test --webpack-config webpack.config.test.js --timeout 30000"
  },
  "author": "Jon Soeder <jon@chicago.com> https://github.com/datapimp",
  "license": "MIT",
  "dependencies": {
    "@skypager/helpers-server": "^0.1.0",
    "@skypager/helpers-sheet": "^0.1.3",
    "@skypager/node": "^0.1.2"
  },
  "skypager": {
    "webpack": {
      "build": "webpack.config.js",
      "scopeModules": false
    }
  }
}
