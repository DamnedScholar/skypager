{
  "name": "@skypager/features-module-manager",
  "version": "0.3.0",
  "description": "an observable state machine for a projects node modules",
  "author": "Jon Soeder <jon@chicago.com>",
  "main": "lib/index.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "babel -d lib src --source-maps --comments",
    "postbuild": "skypager hash-build",
    "docs:api": "skypager generate-api-docs",
    "prepare": "yarn docs:api && yarn build",
    "test": "mocha --require @babel/register --file test/test.js --timeout 10000 test/**/*.spec.js"
  },
  "skypager": {
    "webpack": {
      "build": "webpack.config.js"
    },
    "category": "features"
  },
  "keywords": [],
  "contributors": [],
  "dependencies": {
    "api-npm": "^1.0.0",
    "npmjs-api": "^1.0.3"
  }
}
