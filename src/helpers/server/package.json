{
  "name": "@skypager/helpers-server",
  "version": "0.3.0",
  "description": "skypager server helper",
  "author": "Jon Soeder <jon@chicago.com>",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepare": "yarn docs:api && yarn build",
    "build": "rimraf lib && babel --comments --source-maps -d lib src",
    "postbuild": "skypager hash-build",
    "watch": "babel --watch --comments --source-maps -d lib src",
    "test": "skypager test",
    "prepublish": "skypager smoke-test",
    "docs:api": "skypager generate-api-docs"
  },
  "skypager": {
    "webpack": {
      "build": "webpack.config.js"
    },
    "category": "helpers"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.15.2",
    "express-history-api-fallback": "^2.2.1",
    "express-prettify": "^0.1.0",
    "express-winston": "^3.0.1",
    "winston": "^3.1.0"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/skypager/skypager"
  },
  "homepage": "https://skypager.io",
  "gitHead": "4f161bd5a57a5d5969d19cf14b0e712be636467c",
  "keywords": [],
  "contributors": []
}
